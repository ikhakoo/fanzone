<h1>Add a Video</h1>
<div class="container">
<%= link_to "Back to Videos", videos_url %>
<%= form_for @video do |f| %>
  <p>
    <%= f.label :source %><br>
    <%= f.text_field :source %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
</div>