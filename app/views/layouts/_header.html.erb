<div class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
          <div class="navbar-header">
              <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand">FANZone</a>
          </div>
          <center>
              <div class="navbar-collapse collapse" id="navbar-main">
                  <ul class="nav navbar-nav">
                      <li><%= link_to_unless_current('Home', root_path) {  link_to('Home', root_path, class: 'active') } %></li>
                     <li><%= link_to_unless_current('Social Feed', tweets_path) {  link_to('News Feed', tweets_path, class: 'active') } %></li>
                     <li><%= link_to_unless_current('News', entries_path) {  link_to('News', entries_path, class: 'active') } %></li>
                     <li><%= link_to_unless_current('Videos', videos_path) {  link_to('Videos', videos_path, class: 'active') } %></li>
                     <li><%= link_to_unless_current('Users', users_path) {  link_to('Users', users_path, class: 'active') } %></li>
                  </ul>
                  <% if signed_in? %>
                  <ul class="nav navbar-nav navbar-right">
                  <li><%= link_to_unless_current('Profile', edit_user_path(current_user)) {  link_to('Profile', edit_user_path(current_user), class: 'active' ) } %></li>
                  </ul>
                    <p class="navbar-text navbar-right">Signed in as <%= current_user.name %> <%= link_to "Log out", session_path("current"), :method => :delete %></p>
